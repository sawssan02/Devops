pipeline {
    agent any

    stages {
        stage('Start') {
            steps {
                echo 'Début du pipeline Jenkins'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Exécution de l\'étape de test'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Déploiement en cours'
            }
        }
        
        stage('End') {
            steps {
                echo 'Fin du pipeline Jenkins'
            }
        }
    }
}
